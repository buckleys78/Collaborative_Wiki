<p id="notice"><%= notice %></p>

<p>
  <strong>Contributions:</strong>
    <table>
      <thead>
        <tr>
          <th>Questions</th>
          <th colspan="3"></th>
        </tr>
      </thead>

      <tbody>
        <% @user.questions.each do |question| %>
          <tr>
            <td><%= question.question %></td>
            <td><%= question.updated_at %></td>
          </tr>
        <% end %>
      </tbody>
    </table>
</p>


<%= link_to 'Edit', edit_user_path(@user) %> |
<%= link_to 'Back', users_path %>
